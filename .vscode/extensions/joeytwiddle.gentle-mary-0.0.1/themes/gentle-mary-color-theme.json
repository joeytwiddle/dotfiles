{
  "// A tool to help tweak a theme": "https://themes.vscode.one/",
  "// Basic configurable colors": "https://code.visualstudio.com/api/references/theme-color",
  "// A big list of color keys": "https://gist.github.com/dcts/5b2af4c8b6918e7d35c4121f11d49fb1",
  "$schema": "vscode://schemas/color-theme",
  "type": "dark",
  "name": "Gentle Mary",
  "tokenColors": [
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#CCB385"
      }
    },
    {
      "name": "Boolean",
      "scope": "constant.language.boolean",
      "settings": {
        "foreground": "#88CCCC"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric, constant.dec.numeric.json5, constant.hex.numeric.json5",
      "settings": {
        "foreground": "#88CCCC"
      }
    },
    {
      "name": "Identifier",
      "scope": "variable, support.variable, support.class, support.constant, meta.definition.variable, entity.name.function",
      "settings": {
        "foreground": "#CCCCCC"
      }
    },
    // Function parameters
    {
      "name": "Variable",
      "scope": "variable.parameter, variable.other",
      "settings": {
        "foreground": "#CCCCCC"
      }
    },
    /*
    {
      "name": "Identifier",
      "scope": "variable.parameter, variable.other.readwrite",
      "settings": {
        //"foreground": "#ee9933"
        //"foreground": "#dd8866"
        "foreground": "#88eeee"
      }
    },
    */
    {
      "name": "Keyword",
      "scope": "keyword, modifier, variable.language.this, support.type.object, constant.language, keyword.operator.expression",
      "settings": {
        "foreground": "#AF4A67",
        "fontStyle": "bold"
      }
    },
    {
      // This is a departure from the WebStorm theme
      "name": "Keyword",
      "scope": "variable.language.this",
      "settings": {
        "foreground": "#A576A0",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Brackets",
      "scope": "meta.brace",
      "settings": {
        "foreground": "#CCCCCC"
      }
    },
    {
      "name": "General punctuation",
      "scope": "punctuation.separator.key-value.js, punctuation.separator.key-value.ts, punctuation.definition.block, punctuation.section.braces",
      "settings": {
        "foreground": "#AAAAAA",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Function call",
      "scope": "entity.name.function, support.function",
      "settings": {
        "foreground": "#3DA676"
      }
    },
    {
      "name": "Storage",
      "scope": "storage.type",
      "settings": {
        "foreground": "#AF4A67",
        //"foreground": "#A576A0",
        "fontStyle": "bold"
      }
    },
    // storage.type.function.ts, meta.function.ts
    {
      "name": "Function keyword",
      "scope": "storage.type.function",
      "settings": {
        "foreground": "#A576A0"
        //"fontStyle": "bold"
      }
    },
    // E.g. async static final
    // meta.function.ts
    // storage.modifier.async.ts
    {
      "name": "Function meta",
      "scope": "storage.modifier",
      "settings": {
        "foreground": "#A576A0"
      }
    },
    {
      "name": "Export / import",
      "scope": "keyword.control.export, meta.import",
      "settings": {
        "foreground": "#AF4A67"
      }
    },
    {
      "name": "Modules",
      "scope": "support.module, support.node",
      "settings": {
        "foreground": "#CCCCCC",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Type",
      "scope": "support.type",
      "settings": {
        "foreground": "#A576A0"
      }
    },
    {
      "name": "ReScript React Component",
      "scope": "source.rescript punctuation.definition.tag entity.name.class",
      "settings": {
        "foreground": "#3DA676"
      }
    },
    {
      "name": "ReScript Class Name",
      "scope": "source.rescript entity.name.class",
      "settings": {
        "foreground": "#A576A0"
      }
    },
    {
      "name": "ReScript Enum Member",
      "scope": "source.rescript variable.other.enummember",
      "settings": {
        "foreground": "#A576A0"
      }
    },
    {
      "name": "ReScript Operators (-> and others)",
      /* I wanted to catch just -> syntax, but this catches all standard operators too. */
      "scope": "source.rescript keyword.operator",
      "settings": {
        "foreground": "#dd9900"
      }
    },
    {
      "name": "ReScript Operator (~)",
      "scope": "source.rescript punctuation.definition.keyword",
      "settings": {
        "foreground": "#dd9900"
      }
    },    {
      "name": "Global",
      "scope": "support.class, support.constant",
      "settings": {
        "foreground": "#CCCCCC"
      }
    },
    {
      "name": "Type",
      "scope": "entity.name.type, entity.other.inherited-class",
      "settings": {
        "foreground": "#A576A0"
      }
    },
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "foreground": "#777777"
      }
    },
    /*
    {
      "name": "Python Quoted Docstring",
      "scope": "string.quoted.docstring.multi.python",
      "settings": {
        "foreground": "#777777"
      }
    },
    */
    {
      "name": "Class",
      "scope": "entity.name.type.class",
      "settings": {
        "foreground": "#3DA676"
      }
    },
    {
      "name": "Class property",
      "scope": "variable.object.property, variable.other.property, variable.other.object.property",
      "settings": {
        "foreground": "#4B95DC"
      }
    },
    {
      "name": "Class method",
      "scope": "meta.definition.method",
      "settings": {
        "foreground": "#3DA676"
      }
    },
    {
      "name": "Function definition",
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#3DA676"
      }
    },
    {
      "name": "Template expression",
      "scope": "template.expression.begin, template.expression.end",
      "settings": {
        "foreground": "#AF4A67",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Template expression interpolation",
      "scope": "punctuation.definition.template-expression.*.*, punctuation.definition.template-expression",
      "settings": {
        "foreground": "#AF4A67",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Operator",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#CCCCCC",
        "fontStyle": ""
      }
    },
    {
      "name": "JavaScript semicolon",
      "scope": "punctuation.terminator.statement",
      "settings": {
        "foreground": "#8A8A8A"
      }
    },
    {
      "name": "TypeScript type definition",
      "scope": "keyword.operator.type.annotation.ts",
      "settings": {
        "foreground": "#CCCCCC"
      }
    },
    {
      "name": "YAML key",
      "scope": "entity.name.tag.yaml",
      "settings": {
        "foreground": "#4B95DC"
      }
    },
    {
      "name": "Modifier",
      "scope": "modifier",
      "settings": {
        "foreground": "#CCCCCC"
      }
    },
    {
      "name": "JSON key",
      "scope": "meta.object-literal.key, meta.object-literal.key string, support.type.property-name.json, string.key.json5, variable.name.tag.json5.key",
      "settings": {
        "foreground": "#4B95DC"
      }
    },
    {
      "name": "JSON constant",
      "scope": "constant.language.json",
      "settings": {
        "foreground": "#AF4A67",
        "fontStyle": "bold"
      }
    },
    {
      "name": "CSS tag",
      "scope": "entity.name.tag.css",
      "settings": {
        "foreground": "#AF4A67"
      }
    },
    {
      "name": "CSS class",
      "scope": "entity.other.attribute-name.class",
      "settings": {
        "foreground": "#3DA676"
      }
    },
    {
      "name": "CSS ID",
      "scope": "entity.other.attribute-name.id",
      "settings": {
        "foreground": "#CCCCCC"
      }
    },
    {
      "name": "CSS property",
      "scope": "support.type.property-name.css",
      "settings": {
        //"foreground": "#4B95DC"
        "foreground": "#A576A0"
      }
    },
    {
      "name": "HTML tag outer",
      "scope": "meta.tag, punctuation.definition.tag",
      "settings": {
        "foreground": "#A576A0"
      }
    },
    {
      "name": "HTML tag inner",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#A576A0"
      }
    },
    {
      "name": "HTML tag attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#3DA676"
      }
    },
    {
      "name": "HTML tag attribute punctuation",
      "scope": "meta.tag.any, meta.tag.inline.any",
      "settings": {
        "foreground": "#CCCCCC"
      }
    },
    {
      "name": "HTML tag attribute punctuation (special cases)",
      "scope": "meta.tag.metadata.script.html, meta.tag.structure.any.html",
      "settings": {
        "foreground": "#CCCCCC"
      }
    },
    {
      "name": "HTML content",
      "scope": "text.html.basic, meta.jsx.children.js, meta.jsx.children.ts",
      "settings": {
        "foreground": "#CCCCCC"
      }
    },
    {
      "name": "JSX tag attribute equals",
      "scope": "meta.tag.attribute keyword.operator.assignment",
      "settings": {
        "foreground": "#CCCCCC"
      }
    },
    {
      "name": "JSX tag attribute curly brackets",
      "scope": "punctuation.section.embedded",
      "settings": {
        "foreground": "#EEEEEE"
        //"foreground": "#CCCCCC"
        //"foreground": "#999999"
        //"foreground": "#AF4A67"
        //"fontStyle": "bold"
      }
    },
    {
      "name": "Markdown heading",
      "scope": "markup.heading",
      "settings": {
        "foreground": "#3DA676",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown link title",
      "scope": "string.other.link.title.markdown",
      "settings": {
        "foreground": "#A576A0"
      }
    },
    {
      "name": "Markdown link punctuation",
      "scope": "punctuation.definition.string.begin.markdown, punctuation.definition.string.end.markdown, punctuation.definition.metadata.markdown",
      "settings": {
        "foreground": "#CCCCCC"
      }
    },
    {
      "name": "Markdown link url",
      "scope": "text.html.markdown meta.link.inline, meta.link.reference",
      "settings": {
        "foreground": "#4B95DC"
      }
    },
    {
      "name": "Markdown list item",
      "scope": "text.html.markdown beginning.punctuation.definition.list",
      "settings": {
        "foreground": "#AF4A67",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown italic",
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown bold",
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown bold italic",
      "scope": "markup.bold markup.italic, markup.italic markup.bold",
      "settings": {
        "fontStyle": "italic bold"
      }
    },
    {
      "name": "Markdown inline code",
      "scope": "markup.inline.raw.string.markdown",
      "settings": {
        "background": "#000000",
        "foreground": "#777777"
      }
    },
    {
      "name": "Markdown code block",
      "scope": "markup.fenced_code.block.markdown",
      "settings": {
        "background": "#000000",
        "foreground": "#777777"
      }
    },
    {
      "name": "Markdown inset quote",
      "scope": "markup.quote.markdown",
      "settings": {
        "foreground": "#CCB385"
      }
    },
    {
      "name": "INI property name",
      "scope": "keyword.other.definition.ini",
      "settings": {
        "foreground": "#CCCCCC"
      }
    },
    {
      "name": "INI section title",
      "scope": "entity.name.section.group-title.ini",
      "settings": {
        "foreground": "#AF4A67",
        "fontStyle": "bold"
      }
    },
    {
      "name": "C# class",
      "scope": "source.cs meta.class.identifier storage.type",
      "settings": {
        "foreground": "#3DA676"
      }
    },
    {
      "name": "C# class method",
      "scope": "source.cs meta.method.identifier entity.name.function",
      "settings": {
        "foreground": "#4B95DC"
      }
    },
    {
      "name": "C# function call",
      "scope": "source.cs meta.method-call meta.method, source.cs entity.name.function",
      "settings": {
        "foreground": "#3DA676"
      }
    },
    {
      "name": "C# type",
      "scope": "source.cs storage.type",
      "settings": {
        "foreground": "#A576A0"
      }
    },
    {
      "name": "C# return type",
      "scope": "source.cs meta.method.return-type",
      "settings": {
        "foreground": "#A576A0"
      }
    },
    {
      "name": "C# preprocessor",
      "scope": "source.cs meta.preprocessor",
      "settings": {
        "foreground": "#777777"
      }
    },
    {
      "name": "C# namespace",
      "scope": "source.cs entity.name.type.namespace",
      "settings": {
        "foreground": "#CCCCCC"
      }
    },
    {
      "name": "shell builtin",
      "scope": "source.shell support.function.builtin.shell",
      "settings": {
        "foreground": "#AF4A67"
      }
    },
    // The following shell colours were written for Jeff Hyklin's "Better Shell Syntax" not the default syntax scopes (which were limited in October 2019)
    // Although even this fails to highlight variable expansion inside strings, and option arguments after a \ line escape.
    {
      "name": "shell command",
      "scope": "source.shell entity.name.command.shell",
      "settings": {
        "foreground": "#3DA676"
      }
    },
    {
      "name": "shell argument",
      "scope": "source.shell string.unquoted.argument.shell",
      "settings": {
        "foreground": "#CCCCCC"
      }
    },
    {
      "name": "shell option",
      "scope": "source.shell constant.other.option.shell",
      "settings": {
        "foreground": "#4B95DC"
      }
    },
    // This wasn't working on variables declared as locals.  Better to not use it than have only some variables highlighted.
    /*
    {
      "name": "shell variable",
      "scope": "source.shell variable.other.assignment.shell, source.shell variable.other.normal.shell",
      "settings": {
        "foreground": "#88CCCC"
      }
    },
    */
    {
      "scope": "token.info-token",
      "settings": {
        "foreground": "#6796e6"
      }
    },
    {
      "scope": "token.warn-token",
      "settings": {
        "foreground": "#cd9731"
      }
    },
    {
      "scope": "token.error-token",
      "settings": {
        "foreground": "#f44747"
      }
    },
    {
      "scope": "token.debug-token",
      "settings": {
        "foreground": "#b267e6"
      }
    },

    /* 2021 tweaks */
    {
      "name": "JSX component",
      "scope": "support.class.component",
      "settings": {
        "foreground": "#3DA676"
      }
    },
    {
      "name": "JSX attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#4B95DC"
      }
    },
    {
      "name": "HTML tag outer",
      "scope": "punctuation.definition.tag",
      "settings": {
        "foreground": "#999999"
      }
    },

    // When we open the search results in an editor
    {
      "scope": "meta.path.search",
      "settings": {
        "foreground": "#ffffff",
        //"foreground": "#88CCCC",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "meta.resultLinePrefix.lineNumber.search",
      "settings": {
        "foreground": "#777777"
      }
    },
    // Highlight where a variable is defined
    /*
    {
      "name": "Variable definition",
      "scope": "meta.definition.variable.js variable.other.readwrite.js, meta.definition.variable.js variable.other.constant.js, variable.other.constant.js, meta.definition.variable.ts variable.other.readwrite.ts, meta.definition.variable.ts variable.other.constant.ts, variable.other.constant.ts",
      // (I want to do this for variable assignments too, but I found reads were indistinguishable from writes)
      // , variable.other.readwrite.alias.js, variable.other.readwrite.alias.ts
      "settings": {
        // I find this to be a minor but helpful difference on macOS, but too strong on Linux
        //"fontStyle": "bold",
        "foreground": "#e4e4e4"
      }
    },
    */
  ],
